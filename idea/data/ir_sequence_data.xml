<odoo noupdate="0">
        <!-- Sequences for sale.order -->
        <record id="sequence_idea_idea_seq" model="ir.sequence">
            <field name="name">Idea sequence</field>
            <field name="code">idea.idea</field>
            <field name="prefix">IDEA/%(range_year)s/</field>
            <field eval="3" name="padding"></field>
            <field eval="1" name="number_next"></field>
            <field eval="1" name="number_increment"></field>
            <field eval="False" name="company_id"></field>
            <field eval="True" name="use_date_range"></field>
        </record>
     <record id="sequence_voting_idea_seq" model="ir.sequence">
            <field name="name">Voting sequence</field>
            <field name="code">idea.voting</field>
            <field name="prefix">VOTE/%(range_year)s/</field>
            <field eval="3" name="padding"></field>
            <field eval="1" name="number_next"></field>
            <field eval="1" name="number_increment"></field>
            <field eval="False" name="company_id"></field>
            <field eval="True" name="use_date_range"></field>
        </record>
</odoo>
